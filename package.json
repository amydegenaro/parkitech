{
  "name": "parkitech",
  "version": "1.0.0",
  "description": "One stop shopping for park management",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "# lint-staged # un-comment to enable",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.1.2",
    "express": "^4.17.1",
    "express-session": "^1.15.1",
    "history": "^4.10.1",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-map-gl": "^6.1.18",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sequelize": "^6.29.0",
    "socket.io": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "axios-mock-adapter": "^1.9.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0",
    "chai": "^3.5.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^8.4.1",
    "eslint-config-fullstack": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^12.1.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.2.3",
    "supertest": "^3.4.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
